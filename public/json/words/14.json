[
  {
    "id": 1,
    "text": {
      "ar": "لِتُخْرِجَ",
      "en": "so that you may bring out",
      "tr": "litukh'rija",
      "ru": "чтобы ты выводил"
    },
    "aud": "014_001_005",
    "img": "/img/book.png"
  },
  {
    "id": 2,
    "text": {
      "ar": "يَسْتَحِبُّونَ",
      "en": "love more",
      "tr": "yastaḥibbūna",
      "ru": "любят больше"
    },
    "aud": "014_003_002",
    "img": "/img/book.png"
  },
  {
    "id": 3,
    "text": {
      "ar": "وَيَبْغُونَهَا",
      "en": "and seek in it",
      "tr": "wayabghūnahā",
      "ru": "желая его"
    },
    "aud": "014_003_011",
    "img": "/img/book.png"
  },
  {
    "id": 4,
    "text": {
      "ar": "بِلِسَانِ",
      "en": "with the language",
      "tr": "bilisāni",
      "ru": "с языком"
    },
    "aud": "014_004_006",
    "img": "/img/book.png"
  },
  {
    "id": 5,
    "text": {
      "ar": "فَيُضِلُّ",
      "en": "Then Allah lets go astray",
      "tr": "fayuḍillu",
      "ru": "И вводит в заблуждение"
    },
    "aud": "014_004_010",
    "img": "/img/book.png"
  },
  {
    "id": 6,
    "text": {
      "ar": "أَخْرِجْ",
      "en": "Bring out",
      "tr": "akhrij",
      "ru": "«Выведи"
    },
    "aud": "014_005_006",
    "img": "/img/book.png"
  },
  {
    "id": 7,
    "text": {
      "ar": "وَذَكِّرْهُمْ",
      "en": "And remind them",
      "tr": "wadhakkir'hum",
      "ru": "и напомни им"
    },
    "aud": "014_005_012",
    "img": "/img/book.png"
  },
  {
    "id": 8,
    "text": {
      "ar": "بِأَيَّامِ",
      "en": "of the days",
      "tr": "bi-ayyāmi",
      "ru": "про дни"
    },
    "aud": "014_005_013",
    "img": "/img/book.png"
  },
  {
    "id": 9,
    "text": {
      "ar": "أَنْجَاكُمْ",
      "en": "He saved you",
      "tr": "anjākum",
      "ru": "Он спас вас"
    },
    "aud": "014_006_010",
    "img": "/img/book.png"
  },
  {
    "id": 10,
    "text": {
      "ar": "يَسُومُونَكُمْ",
      "en": "they were afflicting you",
      "tr": "yasūmūnakum",
      "ru": "возлагая на вас"
    },
    "aud": "014_006_014",
    "img": "/img/book.png"
  },
  {
    "id": 11,
    "text": {
      "ar": "وَيُذَبِّحُونَ",
      "en": "and were slaughtering",
      "tr": "wayudhabbiḥūna",
      "ru": "вырезая"
    },
    "aud": "014_006_017",
    "img": "/img/book.png"
  },
  {
    "id": 12,
    "text": {
      "ar": "وَيَسْتَحْيُونَ",
      "en": "and letting live",
      "tr": "wayastaḥyūna",
      "ru": "оставляя в живых"
    },
    "aud": "014_006_019",
    "img": "/img/book.png"
  },
  {
    "id": 13,
    "text": {
      "ar": "نِسَاءَكُمْ",
      "en": "your women",
      "tr": "nisāakum",
      "ru": "ваших женщин."
    },
    "aud": "014_006_020",
    "img": "/img/book.png"
  },
  {
    "id": 14,
    "text": {
      "ar": "بَلَاءٌ",
      "en": "(was) a trial",
      "tr": "balāon",
      "ru": "испытание"
    },
    "aud": "014_006_023",
    "img": "/img/book.png"
  },
  {
    "id": 15,
    "text": {
      "ar": "تَأَذَّنَ",
      "en": "proclaimed",
      "tr": "ta-adhana",
      "ru": "возвестил"
    },
    "aud": "014_007_002",
    "img": "/img/book.png"
  },
  {
    "id": 16,
    "text": {
      "ar": "شَكَرْتُمْ",
      "en": "you are thankful",
      "tr": "shakartum",
      "ru": "вы будете благодарными,"
    },
    "aud": "014_007_005",
    "img": "/img/book.png"
  },
  {
    "id": 17,
    "text": {
      "ar": "لَأَزِيدَنَّكُمْ",
      "en": "surely I will increase you",
      "tr": "la-azīdannakum",
      "ru": "то Я непременно умножу вам,"
    },
    "aud": "014_007_006",
    "img": "/img/book.png"
  },
  {
    "id": 18,
    "text": {
      "ar": "فَرَدُّوا",
      "en": "but they returned",
      "tr": "faraddū",
      "ru": "а они вернули"
    },
    "aud": "014_009_021",
    "img": "/img/book.png"
  },
  {
    "id": 19,
    "text": {
      "ar": "تَدْعُونَنَا",
      "en": "you invite us",
      "tr": "tadʿūnanā",
      "ru": "вы нас призываете"
    },
    "aud": "014_009_035",
    "img": "/img/book.png"
  },
  {
    "id": 20,
    "text": {
      "ar": "شَكٌّ",
      "en": "any doubt",
      "tr": "shakkun",
      "ru": "(есть) сомнение"
    },
    "aud": "014_010_005",
    "img": "/img/book.png"
  },
  {
    "id": 21,
    "text": {
      "ar": "وَيُؤَخِّرَكُمْ",
      "en": "and give you respite",
      "tr": "wayu-akhirakum",
      "ru": "и отсрочить вам"
    },
    "aud": "014_010_014",
    "img": "/img/book.png"
  },
  {
    "id": 22,
    "text": {
      "ar": "تَصُدُّونَا",
      "en": "hinder us",
      "tr": "taṣuddūnā",
      "ru": "отвратить нас"
    },
    "aud": "014_010_026",
    "img": "/img/book.png"
  },
  {
    "id": 23,
    "text": {
      "ar": "فَأْتُونَا",
      "en": "So bring us",
      "tr": "fatūnā",
      "ru": "Приве­дите же нам"
    },
    "aud": "014_010_031",
    "img": "/img/book.png"
  },
  {
    "id": 24,
    "text": {
      "ar": "نَأْتِيَكُمْ",
      "en": "we bring you",
      "tr": "natiyakum",
      "ru": "приводили мы вам"
    },
    "aud": "014_011_021",
    "img": "/img/book.png"
  },
  {
    "id": 25,
    "text": {
      "ar": "نَتَوَكَّلَ",
      "en": "we put our trust",
      "tr": "natawakkala",
      "ru": "полагались мы"
    },
    "aud": "014_012_004",
    "img": "/img/book.png"
  },
  {
    "id": 26,
    "text": {
      "ar": "هَدَانَا",
      "en": "He has guided us",
      "tr": "hadānā",
      "ru": "Он повел нас"
    },
    "aud": "014_012_008",
    "img": "/img/book.png"
  },
  {
    "id": 27,
    "text": {
      "ar": "وَلَنَصْبِرَنَّ",
      "en": "And surely we will bear with patience",
      "tr": "walanaṣbiranna",
      "ru": "И мы непременно будем терпеть"
    },
    "aud": "014_012_010",
    "img": "/img/book.png"
  },
  {
    "id": 28,
    "text": {
      "ar": "ءَاذَيْتُمُونَا",
      "en": "harm you may cause us",
      "tr": "ādhaytumūnā",
      "ru": "вы причиняете нам обиду"
    },
    "aud": "014_012_013",
    "img": "/img/book.png"
  },
  {
    "id": 29,
    "text": {
      "ar": "لِرُسُلِهِمْ",
      "en": "to their Messengers",
      "tr": "lirusulihim",
      "ru": "своим посланникам:"
    },
    "aud": "014_013_004",
    "img": "/img/book.png"
  },
  {
    "id": 30,
    "text": {
      "ar": "لَنُخْرِجَنَّكُمْ",
      "en": "Surely we will drive you out",
      "tr": "lanukh'rijannakum",
      "ru": "«Непременно выпроводим мы вас"
    },
    "aud": "014_013_005",
    "img": "/img/book.png"
  },
  {
    "id": 31,
    "text": {
      "ar": "لَتَعُودُنَّ",
      "en": "surely you should return",
      "tr": "lataʿūdunna",
      "ru": "непременно вернетесь вы обратно"
    },
    "aud": "014_013_009",
    "img": "/img/book.png"
  },
  {
    "id": 32,
    "text": {
      "ar": "مِلَّتِنَا",
      "en": "our religion",
      "tr": "millatinā",
      "ru": "нашему верованию»."
    },
    "aud": "014_013_011",
    "img": "/img/book.png"
  },
  {
    "id": 33,
    "text": {
      "ar": "لَنُهْلِكَنَّ",
      "en": "We will surely destroy",
      "tr": "lanuh'likanna",
      "ru": "«Непременно погу­бим Мы"
    },
    "aud": "014_013_015",
    "img": "/img/book.png"
  },
  {
    "id": 34,
    "text": {
      "ar": "وَلَنُسْكِنَنَّكُمُ",
      "en": "And surely We will make you dwell",
      "tr": "walanus'kinannakumu",
      "ru": "и обязательно поселим Мы вас"
    },
    "aud": "014_014_001",
    "img": "/img/book.png"
  },
  {
    "id": 35,
    "text": {
      "ar": "مَقَامِي",
      "en": "standing before Me",
      "tr": "maqāmī",
      "ru": "стояния предо Мной"
    },
    "aud": "014_014_008",
    "img": "/img/book.png"
  },
  {
    "id": 36,
    "text": {
      "ar": "وَخَافَ",
      "en": "and fears",
      "tr": "wakhāfa",
      "ru": "и боится"
    },
    "aud": "014_014_009",
    "img": "/img/book.png"
  },
  {
    "id": 37,
    "text": {
      "ar": "وَاسْتَفْتَحُوا",
      "en": "And they sought victory",
      "tr": "wa-is'taftaḥū",
      "ru": "И просили они для себя победы"
    },
    "aud": "014_015_001",
    "img": "/img/book.png"
  },
  {
    "id": 38,
    "text": {
      "ar": "وَخَابَ",
      "en": "and disappointed",
      "tr": "wakhāba",
      "ru": "и оказывался в убытке"
    },
    "aud": "014_015_002",
    "img": "/img/book.png"
  },
  {
    "id": 39,
    "text": {
      "ar": "وَرَائِهِ",
      "en": "Ahead of him",
      "tr": "warāihi",
      "ru": "его –"
    },
    "aud": "014_016_002",
    "img": "/img/book.png"
  },
  {
    "id": 40,
    "text": {
      "ar": "وَيُسْقَى",
      "en": "and he will be made to drink",
      "tr": "wayus'qā",
      "ru": "и будут поить его"
    },
    "aud": "014_016_004",
    "img": "/img/book.png"
  },
  {
    "id": 41,
    "text": {
      "ar": "صَدِيدٍ",
      "en": "purulent",
      "tr": "ṣadīdin",
      "ru": "гнойной"
    },
    "aud": "014_016_007",
    "img": "/img/book.png"
  },
  {
    "id": 42,
    "text": {
      "ar": "يَتَجَرَّعُهُ",
      "en": "He will sip it",
      "tr": "yatajarraʿuhu",
      "ru": "Он будет пить ее глотками,"
    },
    "aud": "014_017_001",
    "img": "/img/book.png"
  },
  {
    "id": 43,
    "text": {
      "ar": "يُسِيغُهُ",
      "en": "(to) swallowing it",
      "tr": "yusīghuhu",
      "ru": "проглатывать ее,"
    },
    "aud": "014_017_004",
    "img": "/img/book.png"
  },
  {
    "id": 44,
    "text": {
      "ar": "وَيَأْتِيهِ",
      "en": "And will come to him",
      "tr": "wayatīhi",
      "ru": "и приходит к нему"
    },
    "aud": "014_017_005",
    "img": "/img/book.png"
  },
  {
    "id": 45,
    "text": {
      "ar": "بِمَيِّتٍ",
      "en": "will die",
      "tr": "bimayyitin",
      "ru": "(таким, что сможет) умереть"
    },
    "aud": "014_017_012",
    "img": "/img/book.png"
  },
  {
    "id": 46,
    "text": {
      "ar": "غَلِيظٌ",
      "en": "harsh",
      "tr": "ghalīẓun",
      "ru": "тяжкое"
    },
    "aud": "014_017_016",
    "img": "/img/book.png"
  },
  {
    "id": 47,
    "text": {
      "ar": "كَرَمَادٍ",
      "en": "(are) like ashes",
      "tr": "karamādin",
      "ru": "подобны пеплу,"
    },
    "aud": "014_018_006",
    "img": "/img/book.png"
  },
  {
    "id": 48,
    "text": {
      "ar": "اشْتَدَّتْ",
      "en": "blows furiously",
      "tr": "ish'taddat",
      "ru": "усилился"
    },
    "aud": "014_018_007",
    "img": "/img/book.png"
  },
  {
    "id": 49,
    "text": {
      "ar": "عَاصِفٍ",
      "en": "stormy",
      "tr": "ʿāṣifin",
      "ru": "ветреный."
    },
    "aud": "014_018_012",
    "img": "/img/book.png"
  },
  {
    "id": 50,
    "text": {
      "ar": "وَبَرَزُوا",
      "en": "And they will come forth",
      "tr": "wabarazū",
      "ru": "И предстанут они"
    },
    "aud": "014_021_001",
    "img": "/img/book.png"
  },
  {
    "id": 51,
    "text": {
      "ar": "لَهَدَيْنَاكُمْ",
      "en": "surely we would have guided you",
      "tr": "lahadaynākum",
      "ru": "то непременно повели бы мы вас"
    },
    "aud": "014_021_025",
    "img": "/img/book.png"
  },
  {
    "id": 52,
    "text": {
      "ar": "أَجَزِعْنَا",
      "en": "whether we show intolerance",
      "tr": "ajaziʿ'nā",
      "ru": "будем ли мы проявлять нетерпение"
    },
    "aud": "014_021_028",
    "img": "/img/book.png"
  },
  {
    "id": 53,
    "text": {
      "ar": "وَعَدَكُمْ",
      "en": "promised you",
      "tr": "waʿadakum",
      "ru": "обещал вам"
    },
    "aud": "014_022_008",
    "img": "/img/book.png"
  },
  {
    "id": 54,
    "text": {
      "ar": "وَوَعَدْتُكُمْ",
      "en": "And I promised you",
      "tr": "wawaʿadttukum",
      "ru": "и я обещал вам,"
    },
    "aud": "014_022_011",
    "img": "/img/book.png"
  },
  {
    "id": 55,
    "text": {
      "ar": "فَأَخْلَفْتُكُمْ",
      "en": "but I betrayed you",
      "tr": "fa-akhlaftukum",
      "ru": "но я обманул вас."
    },
    "aud": "014_022_012",
    "img": "/img/book.png"
  },
  {
    "id": 56,
    "text": {
      "ar": "دَعَوْتُكُمْ",
      "en": "I invited you",
      "tr": "daʿawtukum",
      "ru": "я позвал вас"
    },
    "aud": "014_022_021",
    "img": "/img/book.png"
  },
  {
    "id": 57,
    "text": {
      "ar": "فَاسْتَجَبْتُمْ",
      "en": "and you responded",
      "tr": "fa-is'tajabtum",
      "ru": "и вы ответили"
    },
    "aud": "014_022_022",
    "img": "/img/book.png"
  },
  {
    "id": 58,
    "text": {
      "ar": "تَلُومُونِي",
      "en": "blame me",
      "tr": "talūmūnī",
      "ru": "порицайте меня,"
    },
    "aud": "014_022_025",
    "img": "/img/book.png"
  },
  {
    "id": 59,
    "text": {
      "ar": "وَلُومُوا",
      "en": "but blame",
      "tr": "walūmū",
      "ru": "а порицайте"
    },
    "aud": "014_022_026",
    "img": "/img/book.png"
  },
  {
    "id": 60,
    "text": {
      "ar": "بِمُصْرِخِكُمْ",
      "en": "(be) your helper",
      "tr": "bimuṣ'rikhikum",
      "ru": "могущим прийти вам на помощь"
    },
    "aud": "014_022_030",
    "img": "/img/book.png"
  },
  {
    "id": 61,
    "text": {
      "ar": "بِمُصْرِخِيَّ",
      "en": "(be) my helper",
      "tr": "bimuṣ'rikhiyya",
      "ru": "могущими прийти мне на помощь."
    },
    "aud": "014_022_033",
    "img": "/img/book.png"
  },
  {
    "id": 62,
    "text": {
      "ar": "كَفَرْتُ",
      "en": "deny",
      "tr": "kafartu",
      "ru": "отрекаюсь"
    },
    "aud": "014_022_035",
    "img": "/img/book.png"
  },
  {
    "id": 63,
    "text": {
      "ar": "أَشْرَكْتُمُونِ",
      "en": "your association of me (with Allah)",
      "tr": "ashraktumūni",
      "ru": "вы меня сделали соучастником"
    },
    "aud": "014_022_037",
    "img": "/img/book.png"
  },
  {
    "id": 64,
    "text": {
      "ar": "وَأُدْخِلَ",
      "en": "And will be admitted",
      "tr": "wa-ud'khila",
      "ru": "И введены будут"
    },
    "aud": "014_023_001",
    "img": "/img/book.png"
  },
  {
    "id": 65,
    "text": {
      "ar": "كَشَجَرَةٍ",
      "en": "(is) like a tree",
      "tr": "kashajaratin",
      "ru": "подобно дереву"
    },
    "aud": "014_024_009",
    "img": "/img/book.png"
  },
  {
    "id": 66,
    "text": {
      "ar": "طَيِّبَةٍ",
      "en": "good",
      "tr": "ṭayyibatin",
      "ru": "благому"
    },
    "aud": "014_024_010",
    "img": "/img/book.png"
  },
  {
    "id": 67,
    "text": {
      "ar": "أَصْلُهَا",
      "en": "its root",
      "tr": "aṣluhā",
      "ru": "корень его"
    },
    "aud": "014_024_011",
    "img": "/img/book.png"
  },
  {
    "id": 68,
    "text": {
      "ar": "ثَابِتٌ",
      "en": "(is) firm",
      "tr": "thābitun",
      "ru": "твёрд,"
    },
    "aud": "014_024_012",
    "img": "/img/book.png"
  },
  {
    "id": 69,
    "text": {
      "ar": "وَفَرْعُهَا",
      "en": "and its branches",
      "tr": "wafarʿuhā",
      "ru": "а ветви его –"
    },
    "aud": "014_024_013",
    "img": "/img/book.png"
  },
  {
    "id": 70,
    "text": {
      "ar": "تُؤْتِي",
      "en": "Giving",
      "tr": "tu'tī",
      "ru": "оно приносит"
    },
    "aud": "014_025_001",
    "img": "/img/book.png"
  },
  {
