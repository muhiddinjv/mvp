[
  {
    "id": 1,
    "text": {
      "ar": "الرَّحِيمِ",
      "en": "the Most Merciful",
      "tr": "l-raḥīmi",
      "ru": "Милосердного"
    },
    "aud": "041_002_004",
    "img": "/img/book.png"
  },
  {
    "id": 2,
    "text": {
      "ar": "فُصِّلَتْ",
      "en": "are detailed",
      "tr": "fuṣṣilat",
      "ru": "разъяснены"
    },
    "aud": "041_003_002",
    "img": "/img/book.png"
  },
  {
    "id": 3,
    "text": {
      "ar": "ءَايَاتُهُ",
      "en": "its Verses",
      "tr": "āyātuhu",
      "ru": "аяты которой"
    },
    "aud": "041_003_003",
    "img": "/img/book.png"
  },
  {
    "id": 4,
    "text": {
      "ar": "بَشِيرًا",
      "en": "A giver of glad tidings",
      "tr": "bashīran",
      "ru": "В качестве радующего вестника"
    },
    "aud": "041_004_001",
    "img": "/img/book.png"
  },
  {
    "id": 5,
    "text": {
      "ar": "فَأَعْرَضَ",
      "en": "but turn away",
      "tr": "fa-aʿraḍa",
      "ru": "но отвернулось"
    },
    "aud": "041_004_003",
    "img": "/img/book.png"
  },
  {
    "id": 6,
    "text": {
      "ar": "قُلُوبُنَا",
      "en": "Our hearts",
      "tr": "qulūbunā",
      "ru": "«Сердца наши"
    },
    "aud": "041_005_002",
    "img": "/img/book.png"
  },
  {
    "id": 7,
    "text": {
      "ar": "أَكِنَّةٍ",
      "en": "coverings",
      "tr": "akinnatin",
      "ru": "покровах"
    },
    "aud": "041_005_004",
    "img": "/img/book.png"
  },
  {
    "id": 8,
    "text": {
      "ar": "تَدْعُونَا",
      "en": "you call us",
      "tr": "tadʿūnā",
      "ru": "ты призываешь нас"
    },
    "aud": "041_005_006",
    "img": "/img/book.png"
  },
  {
    "id": 9,
    "text": {
      "ar": "ءَاذَانِنَا",
      "en": "our ears",
      "tr": "ādhāninā",
      "ru": "ушах наших"
    },
    "aud": "041_005_009",
    "img": "/img/book.png"
  },
  {
    "id": 10,
    "text": {
      "ar": "وَقْرٌ",
      "en": "(is) deafness",
      "tr": "waqrun",
      "ru": "глухота"
    },
    "aud": "041_005_010",
    "img": "/img/book.png"
  },
  {
    "id": 11,
    "text": {
      "ar": "وَبَيْنِكَ",
      "en": "and between you",
      "tr": "wabaynika",
      "ru": "и между тобой"
    },
    "aud": "041_005_013",
    "img": "/img/book.png"
  },
  {
    "id": 12,
    "text": {
      "ar": "حِجَابٌ",
      "en": "(is) a screen",
      "tr": "ḥijābun",
      "ru": "завеса"
    },
    "aud": "041_005_014",
    "img": "/img/book.png"
  },
  {
    "id": 13,
    "text": {
      "ar": "فَاعْمَلْ",
      "en": "So work",
      "tr": "fa-iʿ'mal",
      "ru": "действуй же"
    },
    "aud": "041_005_015",
    "img": "/img/book.png"
  },
  {
    "id": 14,
    "text": {
      "ar": "عَامِلُونَ",
      "en": "(are) working",
      "tr": "ʿāmilūna",
      "ru": "действуем (досл действующие)»."
    },
    "aud": "041_005_017",
    "img": "/img/book.png"
  },
  {
    "id": 15,
    "text": {
      "ar": "بَشَرٌ",
      "en": "a man",
      "tr": "basharun",
      "ru": "(лишь) человек"
    },
    "aud": "041_006_004",
    "img": "/img/book.png"
  },
  {
    "id": 16,
    "text": {
      "ar": "مِثْلُكُمْ",
      "en": "like you",
      "tr": "mith'lukum",
      "ru": "такой же, как вы"
    },
    "aud": "041_006_005",
    "img": "/img/book.png"
  },
  {
    "id": 17,
    "text": {
      "ar": "إِلَهُكُمْ",
      "en": "your god",
      "tr": "ilāhukum",
      "ru": "Бог ваш"
    },
    "aud": "041_006_009",
    "img": "/img/book.png"
  },
  {
    "id": 18,
    "text": {
      "ar": "وَاحِدٌ",
      "en": "One",
      "tr": "wāḥidun",
      "ru": "один."
    },
    "aud": "041_006_011",
    "img": "/img/book.png"
  },
  {
    "id": 19,
    "text": {
      "ar": "فَاسْتَقِيمُوا",
      "en": "so take a Straight Path",
      "tr": "fa-is'taqīmū",
      "ru": "Так идите же прямым путем"
    },
    "aud": "041_006_012",
    "img": "/img/book.png"
  },
  {
    "id": 20,
    "text": {
      "ar": "وَاسْتَغْفِرُوهُ",
      "en": "and ask His forgiveness",
      "tr": "wa-is'taghfirūhu",
      "ru": "и просите у Него прощения"
    },
    "aud": "041_006_014",
    "img": "/img/book.png"
  },
  {
    "id": 21,
    "text": {
      "ar": "وَوَيْلٌ",
      "en": "And woe",
      "tr": "wawaylun",
      "ru": "И горе"
    },
    "aud": "041_006_015",
    "img": "/img/book.png"
  },
  {
    "id": 22,
    "text": {
      "ar": "لِلْمُشْرِكِينَ",
      "en": "to the polytheists",
      "tr": "lil'mush'rikīna",
      "ru": "многобожникам"
    },
    "aud": "041_006_016",
    "img": "/img/book.png"
  },
  {
    "id": 23,
    "text": {
      "ar": "يُؤْتُونَ",
      "en": "give",
      "tr": "yu'tūna",
      "ru": "дают"
    },
    "aud": "041_007_003",
    "img": "/img/book.png"
  },
  {
    "id": 24,
    "text": {
      "ar": "أَئِنَّكُمْ",
      "en": "Do you indeed",
      "tr": "a-innakum",
      "ru": "«Неужели вы"
    },
    "aud": "041_009_002",
    "img": "/img/book.png"
  },
  {
    "id": 25,
    "text": {
      "ar": "لَتَكْفُرُونَ",
      "en": "[surely] disbelieve",
      "tr": "latakfurūna",
      "ru": "однозначно, проявляете неверие"
    },
    "aud": "041_009_003",
    "img": "/img/book.png"
  },
  {
    "id": 26,
    "text": {
      "ar": "يَوْمَيْنِ",
      "en": "two periods",
      "tr": "yawmayni",
      "ru": "два дня"
    },
    "aud": "041_009_008",
    "img": "/img/book.png"
  },
  {
    "id": 27,
    "text": {
      "ar": "أَنْدَادًا",
      "en": "rivals",
      "tr": "andādan",
      "ru": "равных?"
    },
    "aud": "041_009_011",
    "img": "/img/book.png"
  },
  {
    "id": 28,
    "text": {
      "ar": "فَوْقِهَا",
      "en": "above it",
      "tr": "fawqihā",
      "ru": "[[MISSING]]"
    },
    "aud": "041_010_005",
    "img": "/img/book.png"
  },
  {
    "id": 29,
    "text": {
      "ar": "وَبَارَكَ",
      "en": "and He blessed",
      "tr": "wabāraka",
      "ru": "и наделил Он благодатью"
    },
    "aud": "041_010_006",
    "img": "/img/book.png"
  },
  {
    "id": 30,
    "text": {
      "ar": "أَقْوَاتَهَا",
      "en": "its sustenance",
      "tr": "aqwātahā",
      "ru": "ее пропитание"
    },
    "aud": "041_010_010",
    "img": "/img/book.png"
  },
  {
    "id": 31,
    "text": {
      "ar": "أَرْبَعَةِ",
      "en": "four",
      "tr": "arbaʿati",
      "ru": "четыре"
    },
    "aud": "041_010_012",
    "img": "/img/book.png"
  },
  {
    "id": 32,
    "text": {
      "ar": "لِلسَّائِلِينَ",
      "en": "for those who ask",
      "tr": "lilssāilīna",
      "ru": "для спрашивающих"
    },
    "aud": "041_010_015",
    "img": "/img/book.png"
  },
  {
    "id": 33,
    "text": {
      "ar": "دُخَانٌ",
      "en": "smoke",
      "tr": "dukhānun",
      "ru": "дымом"
    },
    "aud": "041_011_006",
    "img": "/img/book.png"
  },
  {
    "id": 34,
    "text": {
      "ar": "وَلِلْأَرْضِ",
      "en": "and to the earth",
      "tr": "walil'arḍi",
      "ru": "и земле:"
    },
    "aud": "041_011_009",
    "img": "/img/book.png"
  },
  {
    "id": 35,
    "text": {
      "ar": "ائْتِيَا",
      "en": "Come both of you",
      "tr": "i'tiyā",
      "ru": "«Придите вы двое"
    },
    "aud": "041_011_010",
    "img": "/img/book.png"
  },
  {
    "id": 36,
    "text": {
      "ar": "طَوْعًا",
      "en": "willingly",
      "tr": "ṭawʿan",
      "ru": "добровольно"
    },
    "aud": "041_011_011",
    "img": "/img/book.png"
  },
  {
    "id": 37,
    "text": {
      "ar": "كَرْهًا",
      "en": "unwillingly",
      "tr": "karhan",
      "ru": "против воли»."
    },
    "aud": "041_011_013",
    "img": "/img/book.png"
  },
  {
    "id": 38,
    "text": {
      "ar": "قَالَتَا",
      "en": "They both said",
      "tr": "qālatā",
      "ru": "Сказали они (обе):"
    },
    "aud": "041_011_014",
    "img": "/img/book.png"
  },
  {
    "id": 39,
    "text": {
      "ar": "أَتَيْنَا",
      "en": "We come",
      "tr": "ataynā",
      "ru": "«Пришли мы (двое)"
    },
    "aud": "041_011_015",
    "img": "/img/book.png"
  },
  {
    "id": 40,
    "text": {
      "ar": "طَائِعِينَ",
      "en": "willingly",
      "tr": "ṭāiʿīna",
      "ru": "добровольно»."
    },
    "aud": "041_011_016",
    "img": "/img/book.png"
  },
  {
    "id": 41,
    "text": {
      "ar": "فَقَضَاهُنَّ",
      "en": "Then He completed them",
      "tr": "faqaḍāhunna",
      "ru": "И установил Он их"
    },
    "aud": "041_012_001",
    "img": "/img/book.png"
  },
  {
    "id": 42,
    "text": {
      "ar": "وَأَوْحَى",
      "en": "and He revealed",
      "tr": "wa-awḥā",
      "ru": "и внушил Он"
    },
    "aud": "041_012_006",
    "img": "/img/book.png"
  },
  {
    "id": 43,
    "text": {
      "ar": "سَمَاءٍ",
      "en": "heaven",
      "tr": "samāin",
      "ru": "небе"
    },
    "aud": "041_012_009",
    "img": "/img/book.png"
  },
  {
    "id": 44,
    "text": {
      "ar": "أَمْرَهَا",
      "en": "its affair",
      "tr": "amrahā",
      "ru": "его дело."
    },
    "aud": "041_012_010",
    "img": "/img/book.png"
  },
  {
    "id": 45,
    "text": {
      "ar": "وَزَيَّنَّا",
      "en": "And We adorned",
      "tr": "wazayyannā",
      "ru": "И разукрасили Мы"
    },
    "aud": "041_012_011",
    "img": "/img/book.png"
  },
  {
    "id": 46,
    "text": {
      "ar": "وَحِفْظًا",
      "en": "and (to) guard",
      "tr": "waḥif'ẓan",
      "ru": "и (оберегли Мы его) обереганием"
    },
    "aud": "041_012_015",
    "img": "/img/book.png"
  },
  {
    "id": 47,
    "text": {
      "ar": "تَقْدِيرُ",
      "en": "(is the) Decree",
      "tr": "taqdīru",
      "ru": "установление"
    },
    "aud": "041_012_017",
    "img": "/img/book.png"
  },
  {
    "id": 48,
    "text": {
      "ar": "الْعَلِيمِ",
      "en": "the All-Knower",
      "tr": "l-ʿalīmi",
      "ru": "Знающего."
    },
    "aud": "041_012_019",
    "img": "/img/book.png"
  },
  {
    "id": 49,
    "text": {
      "ar": "أَنْذَرْتُكُمْ",
      "en": "I have warned you",
      "tr": "andhartukum",
      "ru": "«Я предостерегаю вас"
    },
    "aud": "041_013_004",
    "img": "/img/book.png"
  },
  {
    "id": 50,
    "text": {
      "ar": "صَاعِقَةً",
      "en": "(of) a thunderbolt",
      "tr": "ṣāʿiqatan",
      "ru": "о поражающем,"
    },
    "aud": "041_013_005",
    "img": "/img/book.png"
  },
  {
    "id": 51,
    "text": {
      "ar": "صَاعِقَةِ",
      "en": "(the) thunderbolt",
      "tr": "ṣāʿiqati",
      "ru": "поражающему"
    },
    "aud": "041_013_007",
    "img": "/img/book.png"
  },
  {
    "id": 52,
    "text": {
      "ar": "خَلْفِهِمْ",
      "en": "behind them",
      "tr": "khalfihim",
      "ru": "[[MISSING]]"
    },
    "aud": "041_014_008",
    "img": "/img/book.png"
  },
  {
    "id": 53,
    "text": {
      "ar": "لَأَنْزَلَ",
      "en": "surely, He (would have) sent down",
      "tr": "la-anzala",
      "ru": "то, непременно, Он низвел бы"
    },
    "aud": "041_014_017",
    "img": "/img/book.png"
  },
  {
    "id": 54,
    "text": {
      "ar": "فَاسْتَكْبَرُوا",
      "en": "they were arrogant",
      "tr": "fa-is'takbarū",
      "ru": "то они возгордились"
    },
    "aud": "041_015_003",
    "img": "/img/book.png"
  },
  {
    "id": 55,
    "text": {
      "ar": "فَأَرْسَلْنَا",
      "en": "So We sent",
      "tr": "fa-arsalnā",
      "ru": "И наслали Мы"
    },
    "aud": "041_016_001",
    "img": "/img/book.png"
  },
  {
    "id": 56,
    "text": {
      "ar": "نَحِسَاتٍ",
      "en": "(of) misfortune",
      "tr": "naḥisātin",
      "ru": "несчастные,"
    },
    "aud": "041_016_007",
    "img": "/img/book.png"
  },
  {
    "id": 57,
    "text": {
      "ar": "لِنُذِيقَهُمْ",
      "en": "that We may make them taste",
      "tr": "linudhīqahum",
      "ru": "чтобы дали Мы им вкусить"
    },
    "aud": "041_016_008",
    "img": "/img/book.png"
  },
  {
    "id": 58,
    "text": {
      "ar": "الْخِزْيِ",
      "en": "(of) disgrace",
      "tr": "l-khiz'yi",
      "ru": "позора"
    },
    "aud": "041_016_010",
    "img": "/img/book.png"
  },
  {
    "id": 59,
    "text": {
      "ar": "أَخْزَى",
      "en": "(is) more disgracing",
      "tr": "akhzā",
      "ru": "позорнее,"
    },
    "aud": "041_016_016",
    "img": "/img/book.png"
  },
  {
    "id": 60,
    "text": {
      "ar": "فَهَدَيْنَاهُمْ",
      "en": "We guided them",
      "tr": "fahadaynāhum",
      "ru": "то Мы указали им верный путь,"
    },
    "aud": "041_017_003",
    "img": "/img/book.png"
  },
  {
    "id": 61,
    "text": {
      "ar": "فَاسْتَحَبُّوا",
      "en": "but they preferred",
      "tr": "fa-is'taḥabbū",
      "ru": "но они полюбили"
    },
    "aud": "041_017_004",
    "img": "/img/book.png"
  },
  {
    "id": 62,
    "text": {
      "ar": "الْعَمَى",
      "en": "[the] blindness",
      "tr": "l-ʿamā",
      "ru": "слепоту"
    },
    "aud": "041_017_005",
    "img": "/img/book.png"
  },
  {
    "id": 63,
    "text": {
      "ar": "فَأَخَذَتْهُمْ",
      "en": "so seized them",
      "tr": "fa-akhadhathum",
      "ru": "и постиг их"
    },
    "aud": "041_017_008",
    "img": "/img/book.png"
  },
  {
    "id": 64,
    "text": {
      "ar": "صَاعِقَةُ",
      "en": "a thunderbolt",
      "tr": "ṣāʿiqatu",
      "ru": "поражающий удар"
    },
    "aud": "041_017_009",
    "img": "/img/book.png"
  },
  {
    "id": 65,
    "text": {
      "ar": "وَنَجَّيْنَا",
      "en": "And We saved",
      "tr": "wanajjaynā",
      "ru": "И спасли Мы"
    },
    "aud": "041_018_001",
    "img": "/img/book.png"
  },
  {
    "id": 66,
    "text": {
      "ar": "يَتَّقُونَ",
      "en": "fear (Allah)",
      "tr": "yattaqūna",
      "ru": "остерегающимися."
    },
    "aud": "041_018_005",
    "img": "/img/book.png"
  },
  {
    "id": 67,
    "text": {
      "ar": "يُحْشَرُ",
      "en": "will be gathered",
      "tr": "yuḥ'sharu",
      "ru": "будут собраны"
    },
    "aud": "041_019_002",
    "img": "/img/book.png"
  },
  {
    "id": 68,
    "text": {
      "ar": "أَعْدَاءُ",
      "en": "(the) enemies",
      "tr": "aʿdāu",
      "ru": "враги"
    },
    "aud": "041_019_003",
    "img": "/img/book.png"
  },
  {
    "id": 69,
    "text": {
      "ar": "يُوزَعُونَ",
      "en": "will be assembled in rows",
      "tr": "yūzaʿūna",
      "ru": "будут задержаны."
    },
    "aud": "041_019_008",
    "img": "/img/book.png"
  },
  {
    "id": 70,
    "text": {
      "ar": "جَاءُوهَا",
      "en": "they come to it",
      "tr": "jāūhā",
      "ru": "придут они к нему,"
    },
    "aud": "041_020_004",
    "img": "/img/book.png"
  },
  {
    "id": 71,
    "text": {
      "ar": "وَأَبْصَارُهُمْ",
      "en": "and their sight",
      "tr": "wa-abṣāruhum",
      "ru": "их зрение"
    },
    "aud": "041_020_008",
    "img": "/img/book.png"
  },
  {
    "id": 72,
    "text": {
      "ar": "وَجُلُودُهُمْ",
      "en": "and their skins",
      "tr": "wajulūduhum",
      "ru": "и их кожи"
    },
    "aud": "041_020_009",
    "img": "/img/book.png"
