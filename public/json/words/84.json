[
  {
    "id": 1,
    "text": {
      "ar": "السَّمَاءُ",
      "en": "the sky",
      "tr": "l-samāu",
      "ru": "небо"
    },
    "aud": "084_001_002",
    "img": "/img/book.png"
  },
  {
    "id": 2,
    "text": {
      "ar": "انْشَقَّتْ",
      "en": "is split asunder",
      "tr": "inshaqqat",
      "ru": "расколется"
    },
    "aud": "084_001_003",
    "img": "/img/book.png"
  },
  {
    "id": 3,
    "text": {
      "ar": "وَأَذِنَتْ",
      "en": "And has listened",
      "tr": "wa-adhinat",
      "ru": "и повинуется оно"
    },
    "aud": "084_002_001",
    "img": "/img/book.png"
  },
  {
    "id": 4,
    "text": {
      "ar": "لِرَبِّهَا",
      "en": "to its Lord",
      "tr": "lirabbihā",
      "ru": "своему Господу,"
    },
    "aud": "084_002_002",
    "img": "/img/book.png"
  },
  {
    "id": 5,
    "text": {
      "ar": "وَحُقَّتْ",
      "en": "and was obligated",
      "tr": "waḥuqqat",
      "ru": "и надлежит ей (исполнить свою обязанность),"
    },
    "aud": "084_002_003",
    "img": "/img/book.png"
  },
  {
    "id": 6,
    "text": {
      "ar": "وَإِذَا",
      "en": "And when",
      "tr": "wa-idhā",
      "ru": "и когда"
    },
    "aud": "084_003_001",
    "img": "/img/book.png"
  },
  {
    "id": 7,
    "text": {
      "ar": "مُدَّتْ",
      "en": "is spread",
      "tr": "muddat",
      "ru": "будет растянута,"
    },
    "aud": "084_003_003",
    "img": "/img/book.png"
  },
  {
    "id": 8,
    "text": {
      "ar": "وَأَلْقَتْ",
      "en": "And has cast out",
      "tr": "wa-alqat",
      "ru": "и извергнет она"
    },
    "aud": "084_004_001",
    "img": "/img/book.png"
  },
  {
    "id": 9,
    "text": {
      "ar": "وَتَخَلَّتْ",
      "en": "and becomes empty",
      "tr": "watakhallat",
      "ru": "и избавится (от умерших),"
    },
    "aud": "084_004_004",
    "img": "/img/book.png"
  },
  {
    "id": 10,
    "text": {
      "ar": "إِنَّكَ",
      "en": "Indeed, you",
      "tr": "innaka",
      "ru": "Поистине, ты"
    },
    "aud": "084_006_003",
    "img": "/img/book.png"
  },
  {
    "id": 11,
    "text": {
      "ar": "كَادِحٌ",
      "en": "(are) laboring",
      "tr": "kādiḥun",
      "ru": "стремящийся"
    },
    "aud": "084_006_004",
    "img": "/img/book.png"
  },
  {
    "id": 12,
    "text": {
      "ar": "كَدْحًا",
      "en": "(with) exertion",
      "tr": "kadḥan",
      "ru": "устремлением"
    },
    "aud": "084_006_007",
    "img": "/img/book.png"
  },
  {
    "id": 13,
    "text": {
      "ar": "فَمُلَاقِيهِ",
      "en": "and you (will) meet Him",
      "tr": "famulāqīhi",
      "ru": "и (затем) встретишь ты Его,"
    },
    "aud": "084_006_008",
    "img": "/img/book.png"
  },
  {
    "id": 14,
    "text": {
      "ar": "أُوتِيَ",
      "en": "is given",
      "tr": "ūtiya",
      "ru": "будет дана"
    },
    "aud": "084_007_003",
    "img": "/img/book.png"
  },
  {
    "id": 15,
    "text": {
      "ar": "كِتَابَهُ",
      "en": "his record",
      "tr": "kitābahu",
      "ru": "его книга"
    },
    "aud": "084_007_004",
    "img": "/img/book.png"
  },
  {
    "id": 16,
    "text": {
      "ar": "بِيَمِينِهِ",
      "en": "in his right hand",
      "tr": "biyamīnihi",
      "ru": "в его правую руку,"
    },
    "aud": "084_007_005",
    "img": "/img/book.png"
  },
  {
    "id": 17,
    "text": {
      "ar": "فَسَوْفَ",
      "en": "Soon",
      "tr": "fasawfa",
      "ru": "то"
    },
    "aud": "084_008_001",
    "img": "/img/book.png"
  },
  {
    "id": 18,
    "text": {
      "ar": "يُحَاسَبُ",
      "en": "his account will be taken",
      "tr": "yuḥāsabu",
      "ru": "будет он рассчитан"
    },
    "aud": "084_008_002",
    "img": "/img/book.png"
  },
  {
    "id": 19,
    "text": {
      "ar": "حِسَابًا",
      "en": "an account",
      "tr": "ḥisāban",
      "ru": "расчётом"
    },
    "aud": "084_008_003",
    "img": "/img/book.png"
  },
  {
    "id": 20,
    "text": {
      "ar": "يَسِيرًا",
      "en": "easy",
      "tr": "yasīran",
      "ru": "лёгким,"
    },
    "aud": "084_008_004",
    "img": "/img/book.png"
  },
  {
    "id": 21,
    "text": {
      "ar": "وَيَنْقَلِبُ",
      "en": "And he will return",
      "tr": "wayanqalibu",
      "ru": "и вернётся он"
    },
    "aud": "084_009_001",
    "img": "/img/book.png"
  },
  {
    "id": 22,
    "text": {
      "ar": "أَهْلِهِ",
      "en": "his people",
      "tr": "ahlihi",
      "ru": "к своей семье"
    },
    "aud": "084_009_003",
    "img": "/img/book.png"
  },
  {
    "id": 23,
    "text": {
      "ar": "مَسْرُورًا",
      "en": "happily",
      "tr": "masrūran",
      "ru": "обрадованным."
    },
    "aud": "084_009_004",
    "img": "/img/book.png"
  },
  {
    "id": 24,
    "text": {
      "ar": "وَرَاءَ",
      "en": "behind",
      "tr": "warāa",
      "ru": "из-за"
    },
    "aud": "084_010_005",
    "img": "/img/book.png"
  },
  {
    "id": 25,
    "text": {
      "ar": "ظَهْرِهِ",
      "en": "his back",
      "tr": "ẓahrihi",
      "ru": "спины его,"
    },
    "aud": "084_010_006",
    "img": "/img/book.png"
  },
  {
    "id": 26,
    "text": {
      "ar": "يَدْعُوا",
      "en": "he will call",
      "tr": "yadʿū",
      "ru": "станет он звать"
    },
    "aud": "084_011_002",
    "img": "/img/book.png"
  },
  {
    "id": 27,
    "text": {
      "ar": "ثُبُورًا",
      "en": "(for) destruction",
      "tr": "thubūran",
      "ru": "погибель"
    },
    "aud": "084_011_003",
    "img": "/img/book.png"
  },
  {
    "id": 28,
    "text": {
      "ar": "وَيَصْلَى",
      "en": "And he will burn",
      "tr": "wayaṣlā",
      "ru": "и будет он гореть"
    },
    "aud": "084_012_001",
    "img": "/img/book.png"
  },
  {
    "id": 29,
    "text": {
      "ar": "سَعِيرًا",
      "en": "(in) a Blaze",
      "tr": "saʿīran",
      "ru": "в Огне."
    },
    "aud": "084_012_002",
    "img": "/img/book.png"
  },
  {
    "id": 30,
    "text": {
      "ar": "ظَنَّ",
      "en": "(had) thought",
      "tr": "ẓanna",
      "ru": "думал,"
    },
    "aud": "084_014_002",
    "img": "/img/book.png"
  },
  {
    "id": 31,
    "text": {
      "ar": "يَحُورَ",
      "en": "he would return",
      "tr": "yaḥūra",
      "ru": "вернётся обратно"
    },
    "aud": "084_014_005",
    "img": "/img/book.png"
  },
  {
    "id": 32,
    "text": {
      "ar": "بَلَى",
      "en": "Nay!",
      "tr": "balā",
      "ru": "Но нет же,"
    },
    "aud": "084_015_001",
    "img": "/img/book.png"
  },
  {
    "id": 33,
    "text": {
      "ar": "بَصِيرًا",
      "en": "seeing",
      "tr": "baṣīran",
      "ru": "видящим."
    },
    "aud": "084_015_006",
    "img": "/img/book.png"
  },
  {
    "id": 34,
    "text": {
      "ar": "بِالشَّفَقِ",
      "en": "by the twilight glow",
      "tr": "bil-shafaqi",
      "ru": "вечернею зарёю,"
    },
    "aud": "084_016_003",
    "img": "/img/book.png"
  },
  {
    "id": 35,
    "text": {
      "ar": "وَسَقَ",
      "en": "it envelops",
      "tr": "wasaqa",
      "ru": "она собирает"
    },
    "aud": "084_017_003",
    "img": "/img/book.png"
  },
  {
    "id": 36,
    "text": {
      "ar": "اتَّسَقَ",
      "en": "it becomes full",
      "tr": "ittasaqa",
      "ru": "она становится полной,"
    },
    "aud": "084_018_003",
    "img": "/img/book.png"
  },
  {
    "id": 37,
    "text": {
      "ar": "لَتَرْكَبُنَّ",
      "en": "You will surely embark",
      "tr": "latarkabunna",
      "ru": "вы непременно переходите"
    },
    "aud": "084_019_001",
    "img": "/img/book.png"
  },
  {
    "id": 38,
    "text": {
      "ar": "طَبَقًا",
      "en": "(to) stage",
      "tr": "ṭabaqan",
      "ru": "(в) (некое) состояние"
    },
    "aud": "084_019_002",
    "img": "/img/book.png"
  },
  {
    "id": 39,
    "text": {
      "ar": "طَبَقٍ",
      "en": "stage",
      "tr": "ṭabaqin",
      "ru": "(другого) состояния"
    },
    "aud": "084_019_004",
    "img": "/img/book.png"
  },
  {
    "id": 40,
    "text": {
      "ar": "يُؤْمِنُونَ",
      "en": "they believe",
      "tr": "yu'minūna",
      "ru": "веруют?"
    },
    "aud": "084_020_004",
    "img": "/img/book.png"
  },
  {
    "id": 41,
    "text": {
      "ar": "قُرِئَ",
      "en": "is recited",
      "tr": "quri-a",
      "ru": "читается"
    },
    "aud": "084_021_002",
    "img": "/img/book.png"
  },
  {
    "id": 42,
    "text": {
      "ar": "عَلَيْهِمُ",
      "en": "to them",
      "tr": "ʿalayhimu",
      "ru": "им"
    },
    "aud": "084_021_003",
    "img": "/img/book.png"
  },
  {
    "id": 43,
    "text": {
      "ar": "الْقُرْءَانُ",
      "en": "the Quran",
      "tr": "l-qur'ānu",
      "ru": "Коран,"
    },
    "aud": "084_021_004",
    "img": "/img/book.png"
  },
  {
    "id": 44,
    "text": {
      "ar": "يَسْجُدُونَ",
      "en": "they prostrate",
      "tr": "yasjudūna",
      "ru": "совершают они земной поклон?"
    },
    "aud": "084_021_006",
    "img": "/img/book.png"
  },
  {
    "id": 45,
    "text": {
      "ar": "يُكَذِّبُونَ",
      "en": "deny",
      "tr": "yukadhibūna",
      "ru": "считают ложью"
    },
    "aud": "084_022_004",
    "img": "/img/book.png"
  },
  {
    "id": 46,
    "text": {
      "ar": "أَعْلَمُ",
      "en": "(is) most knowing",
      "tr": "aʿlamu",
      "ru": "лучше знает,"
    },
    "aud": "084_023_002",
    "img": "/img/book.png"
  },
  {
    "id": 47,
    "text": {
      "ar": "بِمَا",
      "en": "of what",
      "tr": "bimā",
      "ru": "что"
    },
    "aud": "084_023_003",
    "img": "/img/book.png"
  },
  {
    "id": 48,
    "text": {
      "ar": "يُوعُونَ",
      "en": "they keep within themselves",
      "tr": "yūʿūna",
      "ru": "они таят в себе"
    },
    "aud": "084_023_004",
    "img": "/img/book.png"
  },
  {
    "id": 49,
    "text": {
      "ar": "فَبَشِّرْهُمْ",
      "en": "So give them tidings",
      "tr": "fabashir'hum",
      "ru": "Обрадуй же их"
    },
    "aud": "084_024_001",
    "img": "/img/book.png"
  },
  {
    "id": 50,
    "text": {
      "ar": "بِعَذَابٍ",
      "en": "of a punishment",
      "tr": "biʿadhābin",
      "ru": "наказанием"
    },
    "aud": "084_024_002",
    "img": "/img/book.png"
  },
  {
    "id": 51,
    "text": {
      "ar": "أَلِيمٍ",
      "en": "painful",
      "tr": "alīmin",
      "ru": "мучительным,"
    },
    "aud": "084_024_003",
    "img": "/img/book.png"
  }
]