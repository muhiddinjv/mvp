[
  {
    "id": 1,
    "text": {
      "ar": "وَالضُّحَى",
      "en": "By the morning brightness",
      "tr": "wal-ḍuḥā",
      "ru": "Клянусь (предполуденным) утром"
    },
    "aud": "093_001_001",
    "img": "/img/book.png"
  },
  {
    "id": 2,
    "text": {
      "ar": "وَاللَّيْلِ",
      "en": "And the night",
      "tr": "wa-al-layli",
      "ru": "и ночью,"
    },
    "aud": "093_002_001",
    "img": "/img/book.png"
  },
  {
    "id": 3,
    "text": {
      "ar": "سَجَى",
      "en": "it covers with darkness",
      "tr": "sajā",
      "ru": "покрывается мраком!"
    },
    "aud": "093_002_003",
    "img": "/img/book.png"
  },
  {
    "id": 4,
    "text": {
      "ar": "وَدَّعَكَ",
      "en": "has forsaken you",
      "tr": "waddaʿaka",
      "ru": "покинул тебя"
    },
    "aud": "093_003_002",
    "img": "/img/book.png"
  },
  {
    "id": 5,
    "text": {
      "ar": "قَلَى",
      "en": "He is displeased",
      "tr": "qalā",
      "ru": "возненавидел."
    },
    "aud": "093_003_005",
    "img": "/img/book.png"
  },
  {
    "id": 6,
    "text": {
      "ar": "وَلَلْءَاخِرَةُ",
      "en": "And surely the Hereafter",
      "tr": "walalākhiratu",
      "ru": "И, однозначно, Последняя (обитель)"
    },
    "aud": "093_004_001",
    "img": "/img/book.png"
  },
  {
    "id": 7,
    "text": {
      "ar": "الْأُولَى",
      "en": "the first",
      "tr": "l-ūlā",
      "ru": "первая (обитель) [чем этот мир]."
    },
    "aud": "093_004_005",
    "img": "/img/book.png"
  },
  {
    "id": 8,
    "text": {
      "ar": "وَلَسَوْفَ",
      "en": "And soon",
      "tr": "walasawfa",
      "ru": "И, однозначно, вскоре"
    },
    "aud": "093_005_001",
    "img": "/img/book.png"
  },
  {
    "id": 9,
    "text": {
      "ar": "يُعْطِيكَ",
      "en": "will give you",
      "tr": "yuʿ'ṭīka",
      "ru": "дарует тебе"
    },
    "aud": "093_005_002",
    "img": "/img/book.png"
  },
  {
    "id": 10,
    "text": {
      "ar": "فَتَرْضَى",
      "en": "then you will be satisfied",
      "tr": "fatarḍā",
      "ru": "и ты будешь доволен."
    },
    "aud": "093_005_004",
    "img": "/img/book.png"
  },
  {
    "id": 11,
    "text": {
      "ar": "يَجِدْكَ",
      "en": "He find you",
      "tr": "yajid'ka",
      "ru": "не нашёл Он тебя"
    },
    "aud": "093_006_002",
    "img": "/img/book.png"
  },
  {
    "id": 12,
    "text": {
      "ar": "يَتِيمًا",
      "en": "an orphan",
      "tr": "yatīman",
      "ru": "сиротой"
    },
    "aud": "093_006_003",
    "img": "/img/book.png"
  },
  {
    "id": 13,
    "text": {
      "ar": "فءَاوَى",
      "en": "and gave shelter",
      "tr": "faāwā",
      "ru": "и приютил?"
    },
    "aud": "093_006_004",
    "img": "/img/book.png"
  },
  {
    "id": 14,
    "text": {
      "ar": "وَوَجَدَكَ",
      "en": "And He found you",
      "tr": "wawajadaka",
      "ru": "И нашёл тебя"
    },
    "aud": "093_007_001",
    "img": "/img/book.png"
  },
  {
    "id": 15,
    "text": {
      "ar": "ضَالًّا",
      "en": "lost",
      "tr": "ḍāllan",
      "ru": "заблудившимся"
    },
    "aud": "093_007_002",
    "img": "/img/book.png"
  },
  {
    "id": 16,
    "text": {
      "ar": "فَهَدَى",
      "en": "so He guided",
      "tr": "fahadā",
      "ru": "и указал тебе путь?"
    },
    "aud": "093_007_003",
    "img": "/img/book.png"
  },
  {
    "id": 17,
    "text": {
      "ar": "عَائِلًا",
      "en": "in need",
      "tr": "ʿāilan",
      "ru": "бедным"
    },
    "aud": "093_008_002",
    "img": "/img/book.png"
  },
  {
    "id": 18,
    "text": {
      "ar": "فَأَغْنَى",
      "en": "so He made self-sufficient",
      "tr": "fa-aghnā",
      "ru": "и обогатил?"
    },
    "aud": "093_008_003",
    "img": "/img/book.png"
  },
  {
    "id": 19,
    "text": {
      "ar": "فَلَا",
      "en": "then (do) not",
      "tr": "falā",
      "ru": "то не"
    },
    "aud": "093_009_003",
    "img": "/img/book.png"
  },
  {
    "id": 20,
    "text": {
      "ar": "تَقْهَرْ",
      "en": "oppress",
      "tr": "taqhar",
      "ru": "притесняй!"
    },
    "aud": "093_009_004",
    "img": "/img/book.png"
  },
  {
    "id": 21,
    "text": {
      "ar": "السَّائِلَ",
      "en": "him who asks",
      "tr": "l-sāila",
      "ru": "просящего,"
    },
    "aud": "093_010_002",
    "img": "/img/book.png"
  },
  {
    "id": 22,
    "text": {
      "ar": "تَنْهَرْ",
      "en": "repel",
      "tr": "tanhar",
      "ru": "отгоняй!"
    },
    "aud": "093_010_004",
    "img": "/img/book.png"
  },
  {
    "id": 23,
    "text": {
      "ar": "بِنِعْمَةِ",
      "en": "(the) Favor",
      "tr": "biniʿ'mati",
      "ru": "благ"
    },
    "aud": "093_011_002",
    "img": "/img/book.png"
  },
  {
    "id": 24,
    "text": {
      "ar": "فَحَدِّثْ",
      "en": "narrate",
      "tr": "faḥaddith",
      "ru": "то рассказывай!"
    },
    "aud": "093_011_004",
    "img": "/img/book.png"
  }
]