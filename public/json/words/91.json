[
  {
    "id": 1,
    "text": {
      "ar": "وَالشَّمْسِ",
      "en": "By the sun",
      "tr": "wal-shamsi",
      "ru": "Клянусь солнцем"
    },
    "aud": "091_001_001",
    "img": "/img/book.png"
  },
  {
    "id": 2,
    "text": {
      "ar": "وَضُحَاهَا",
      "en": "and its brightness",
      "tr": "waḍuḥāhā",
      "ru": "и его сиянием"
    },
    "aud": "091_001_002",
    "img": "/img/book.png"
  },
  {
    "id": 3,
    "text": {
      "ar": "وَالْقَمَرِ",
      "en": "And the moon",
      "tr": "wal-qamari",
      "ru": "и луной,"
    },
    "aud": "091_002_001",
    "img": "/img/book.png"
  },
  {
    "id": 4,
    "text": {
      "ar": "تَلَاهَا",
      "en": "it follows it",
      "tr": "talāhā",
      "ru": "она следует за ним,"
    },
    "aud": "091_002_003",
    "img": "/img/book.png"
  },
  {
    "id": 5,
    "text": {
      "ar": "جَلَّاهَا",
      "en": "it displays it",
      "tr": "jallāhā",
      "ru": "он являет его,"
    },
    "aud": "091_003_003",
    "img": "/img/book.png"
  },
  {
    "id": 6,
    "text": {
      "ar": "يَغْشَاهَا",
      "en": "it covers it",
      "tr": "yaghshāhā",
      "ru": "она покрывает его,"
    },
    "aud": "091_004_003",
    "img": "/img/book.png"
  },
  {
    "id": 7,
    "text": {
      "ar": "وَالسَّمَاءِ",
      "en": "And the heaven",
      "tr": "wal-samāi",
      "ru": "и небом,"
    },
    "aud": "091_005_001",
    "img": "/img/book.png"
  },
  {
    "id": 8,
    "text": {
      "ar": "بَنَاهَا",
      "en": "constructed it",
      "tr": "banāhā",
      "ru": "Кто его построил,"
    },
    "aud": "091_005_003",
    "img": "/img/book.png"
  },
  {
    "id": 9,
    "text": {
      "ar": "وَالْأَرْضِ",
      "en": "And the earth",
      "tr": "wal-arḍi",
      "ru": "и землей,"
    },
    "aud": "091_006_001",
    "img": "/img/book.png"
  },
  {
    "id": 10,
    "text": {
      "ar": "طَحَاهَا",
      "en": "spread it",
      "tr": "ṭaḥāhā",
      "ru": "Кто распростер ее,"
    },
    "aud": "091_006_003",
    "img": "/img/book.png"
  },
  {
    "id": 11,
    "text": {
      "ar": "وَنَفْسٍ",
      "en": "And (the) soul",
      "tr": "wanafsin",
      "ru": "и всякой душой,"
    },
    "aud": "091_007_001",
    "img": "/img/book.png"
  },
  {
    "id": 12,
    "text": {
      "ar": "سَوَّاهَا",
      "en": "proportioned it",
      "tr": "sawwāhā",
      "ru": "Кто её устроил"
    },
    "aud": "091_007_003",
    "img": "/img/book.png"
  },
  {
    "id": 13,
    "text": {
      "ar": "فَأَلْهَمَهَا",
      "en": "And He inspired it",
      "tr": "fa-alhamahā",
      "ru": "и внушил [разъяснил] ей"
    },
    "aud": "091_008_001",
    "img": "/img/book.png"
  },
  {
    "id": 14,
    "text": {
      "ar": "فُجُورَهَا",
      "en": "(to understand) what is wrong for it",
      "tr": "fujūrahā",
      "ru": "её грехолюбие (порочность, распущенность)"
    },
    "aud": "091_008_002",
    "img": "/img/book.png"
  },
  {
    "id": 15,
    "text": {
      "ar": "وَتَقْوَاهَا",
      "en": "and what is right for it",
      "tr": "wataqwāhā",
      "ru": "и её остережение (от грехов)"
    },
    "aud": "091_008_003",
    "img": "/img/book.png"
  },
  {
    "id": 16,
    "text": {
      "ar": "قَدْ",
      "en": "Indeed",
      "tr": "qad",
      "ru": "Действительно"
    },
    "aud": "091_009_001",
    "img": "/img/book.png"
  },
  {
    "id": 17,
    "text": {
      "ar": "أَفْلَحَ",
      "en": "he succeeds",
      "tr": "aflaḥa",
      "ru": "преуспел"
    },
    "aud": "091_009_002",
    "img": "/img/book.png"
  },
  {
    "id": 18,
    "text": {
      "ar": "زَكَّاهَا",
      "en": "purifies it",
      "tr": "zakkāhā",
      "ru": "очистил ее"
    },
    "aud": "091_009_004",
    "img": "/img/book.png"
  },
  {
    "id": 19,
    "text": {
      "ar": "وَقَدْ",
      "en": "And indeed",
      "tr": "waqad",
      "ru": "и действительно"
    },
    "aud": "091_010_001",
    "img": "/img/book.png"
  },
  {
    "id": 20,
    "text": {
      "ar": "خَابَ",
      "en": "he fails",
      "tr": "khāba",
      "ru": "понес урон"
    },
    "aud": "091_010_002",
    "img": "/img/book.png"
  },
  {
    "id": 21,
    "text": {
      "ar": "دَسَّاهَا",
      "en": "corrupts it",
      "tr": "dassāhā",
      "ru": "утаил ее"
    },
    "aud": "091_010_004",
    "img": "/img/book.png"
  },
  {
    "id": 22,
    "text": {
      "ar": "كَذَّبَتْ",
      "en": "Denied",
      "tr": "kadhabat",
      "ru": "Сочли лжецом"
    },
    "aud": "091_011_001",
    "img": "/img/book.png"
  },
  {
    "id": 23,
    "text": {
      "ar": "ثَمُودُ",
      "en": "Thamud",
      "tr": "thamūdu",
      "ru": "самудиты"
    },
    "aud": "091_011_002",
    "img": "/img/book.png"
  },
  {
    "id": 24,
    "text": {
      "ar": "بِطَغْوَاهَا",
      "en": "by their transgression",
      "tr": "biṭaghwāhā",
      "ru": "из-за своего беззакония."
    },
    "aud": "091_011_003",
    "img": "/img/book.png"
  },
  {
    "id": 25,
    "text": {
      "ar": "إِذِ",
      "en": "When",
      "tr": "idhi",
      "ru": "Вот"
    },
    "aud": "091_012_001",
    "img": "/img/book.png"
  },
  {
    "id": 26,
    "text": {
      "ar": "انْبَعَثَ",
      "en": "(was) sent forth",
      "tr": "inbaʿatha",
      "ru": "вызвался"
    },
    "aud": "091_012_002",
    "img": "/img/book.png"
  },
  {
    "id": 27,
    "text": {
      "ar": "أَشْقَاهَا",
      "en": "(the) most wicked of them",
      "tr": "ashqāhā",
      "ru": "несчастнейший из них"
    },
    "aud": "091_012_003",
    "img": "/img/book.png"
  },
  {
    "id": 28,
    "text": {
      "ar": "فَقَالَ",
      "en": "But said",
      "tr": "faqāla",
      "ru": "и сказал"
    },
    "aud": "091_013_001",
    "img": "/img/book.png"
  },
  {
    "id": 29,
    "text": {
      "ar": "لَهُمْ",
      "en": "to them",
      "tr": "lahum",
      "ru": "им"
    },
    "aud": "091_013_002",
    "img": "/img/book.png"
  },
  {
    "id": 30,
    "text": {
      "ar": "رَسُولُ",
      "en": "(the) Messenger",
      "tr": "rasūlu",
      "ru": "посланник"
    },
    "aud": "091_013_003",
    "img": "/img/book.png"
  },
  {
    "id": 31,
    "text": {
      "ar": "نَاقَةَ",
      "en": "(It is the) she-camel",
      "tr": "nāqata",
      "ru": "«(Оставьте) верблюдицу"
    },
    "aud": "091_013_005",
    "img": "/img/book.png"
  },
  {
    "id": 32,
    "text": {
      "ar": "وَسُقْيَاهَا",
      "en": "and her drink",
      "tr": "wasuq'yāhā",
      "ru": "и питьё её!»"
    },
    "aud": "091_013_007",
    "img": "/img/book.png"
  },
  {
    "id": 33,
    "text": {
      "ar": "فَكَذَّبُوهُ",
      "en": "But they denied him",
      "tr": "fakadhabūhu",
      "ru": "И сочли они его лжецом"
    },
    "aud": "091_014_001",
    "img": "/img/book.png"
  },
  {
    "id": 34,
    "text": {
      "ar": "فَعَقَرُوهَا",
      "en": "and they hamstrung her",
      "tr": "faʿaqarūhā",
      "ru": "и подрезали ее"
    },
    "aud": "091_014_002",
    "img": "/img/book.png"
  },
  {
    "id": 35,
    "text": {
      "ar": "فَدَمْدَمَ",
      "en": "So destroyed",
      "tr": "fadamdama",
      "ru": "и истребил"
    },
    "aud": "091_014_003",
    "img": "/img/book.png"
  },
  {
    "id": 36,
    "text": {
      "ar": "رَبُّهُمْ",
      "en": "their Lord",
      "tr": "rabbuhum",
      "ru": "Господь их"
    },
    "aud": "091_014_005",
    "img": "/img/book.png"
  },
  {
    "id": 37,
    "text": {
      "ar": "بِذَنْبِهِمْ",
      "en": "for their sin",
      "tr": "bidhanbihim",
      "ru": "за их грех"
    },
    "aud": "091_014_006",
    "img": "/img/book.png"
  },
  {
    "id": 38,
    "text": {
      "ar": "فَسَوَّاهَا",
      "en": "and leveled them",
      "tr": "fasawwāhā",
      "ru": "и сделал его одинаковым (для всех)"
    },
    "aud": "091_014_007",
    "img": "/img/book.png"
  },
  {
    "id": 39,
    "text": {
      "ar": "يَخَافُ",
      "en": "He fears",
      "tr": "yakhāfu",
      "ru": "боясь"
    },
    "aud": "091_015_002",
    "img": "/img/book.png"
  },
  {
    "id": 40,
    "text": {
      "ar": "عُقْبَاهَا",
      "en": "its consequences",
      "tr": "ʿuq'bāhā",
      "ru": "последствий этого"
    },
    "aud": "091_015_003",
    "img": "/img/book.png"
  }
]